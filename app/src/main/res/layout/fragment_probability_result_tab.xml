<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingVertical="24dp"
    tools:context=".ui.fragments.probabilityTab.ProbabilityResultTabFragment">

    <TextView
        android:id="@+id/tv_acne_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/result_probability" />

    <TextView
        android:id="@+id/tv_blackheads_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="6dp"
        android:text="@string/result_probability" />

    <TextView
        android:id="@+id/tv_hyperpigmentation_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/result_probability" />

    <TextView
        android:id="@+id/tv_oily_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="6dp"
        android:text="@string/result_probability" />

    <TextView
        android:id="@+id/tv_redness_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:text="@string/result_probability" />

    <TextView
        android:id="@+id/tv_normal_result_probability"
        style="@style/PrimaryRegular12Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/result_probability" />

</LinearLayout>